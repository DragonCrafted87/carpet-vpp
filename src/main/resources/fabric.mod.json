{
    "schemaVersion": 1,
    "accessWidener": "vpp.accesswidener",
    "authors": [
        "Me!"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "depends": {
        "fabric": ">=${fabric_version}",
        "minecraft": ">=${minecraft_version}",
        "trinkets": ">=${trinkets_version}"
    },
    "recommends": {
        "emi": ">=${emi_version}"
    },
    "breaks": {
        "infinitory": "*",
        "biginv": "*",
        "extrainv": "*",
        "inventory_backpack": "*",
        "realistic-inventory": "*",
        "inventorymod": "*"
    },
    "description": "This is an example description! Tell everyone what your mod is about!",
    "entrypoints": {
        "client": [
            "dragoncrafted87.vpp.MinecraftVPPClient"
        ],
        "main": [
            "dragoncrafted87.vpp.MinecraftVPP"
        ],
        "server": [
            "dragoncrafted87.vpp.MinecraftVPPServer"
        ],
        "emi": [
            "dragoncrafted87.vpp.compat.MinecraftVPPEmiPlugin"
        ]
    },
    "environment": "*",
    "icon": "assets/minecraft-vpp/icon.png",
    "id": "minecraft-vpp",
    "license": "MIT",
    "mixins": [
        "minecraft-vpp.mixins.json",
        {
            "config": "minecraft-vpp.client.mixins.json",
            "environment": "client"
        }
    ],
    "name": "Vanilla Plus Plus Mod",
    "suggests": {
        "another-mod": "*"
    },
    "version": "${version}"
}
